{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "arrow-body-style": "warn",
    "react/prop-types": 1,
    "react/no-deprecated": 1,
    "prettier/prettier": "error",
    "no-use-before-define": ["error", {"variables": false}]
  }
}
